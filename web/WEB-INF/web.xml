<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>rssServlet</servlet-name>
        <servlet-class>rss.rssServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>rss.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>newuser</servlet-name>
        <servlet-class>rss.newuser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>collection</servlet-name>
        <servlet-class>rss.collection</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>rssServlet2</servlet-name>
        <servlet-class>rss.rssServlet2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>rss.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>saveServlet</servlet-name>
        <servlet-class>rss.saveServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>rssModel</servlet-name>
        <servlet-class>rss.rssModel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>changeColor</servlet-name>
        <servlet-class>rss.changeColor</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>rssServlet</servlet-name>
        <url-pattern>/rss</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>newuser</servlet-name>
        <url-pattern>/newuser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>collection</servlet-name>
        <url-pattern>/collection</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>rssServlet2</servlet-name>
        <url-pattern>/rssServlet2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>saveServlet</servlet-name>
        <url-pattern>/saveServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>rssModel</servlet-name>
        <url-pattern>/rssModel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>changeColor</servlet-name>
        <url-pattern>/changeColor</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>90</session-timeout>
    </session-config>
</web-app>
